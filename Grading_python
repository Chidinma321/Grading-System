score = int(input("Enter your score (0 - 100): "))

if 70 <= score <= 100:
    grade = 'A'
elif 60 <= score <= 69:
    grade = 'B'
elif 50 <= score <= 59:
    grade = 'C'
elif 45 <= score <= 49:
    grade = 'D'
elif 40 <= score <= 44:
    grade = 'E'
elif 0 <= score < 40:
    grade = 'F'
else:
    grade = None

if grade:
    print(f"Your grade is: {grade}")
else:
    print("Invalid score. Please enter a value between 0 and 100.")
